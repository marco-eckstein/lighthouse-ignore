{
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "no-submodule-imports": [
            true,
            "source-map-support/register",
            "lighthouse/lighthouse-core/config/default-config"
        ],
        "no-console": false,
        "trailing-comma": [true, { "multiline": "ignore", "singleline": "never", "esSpecCompliant": true }]
    }
}
